{
  "name": "next-auth-mongodb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:local": "dotenv -e .env.local.dev next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "clean-cache": "rm -rf .next/cache",
    "clean-dist": "rm -rf .next",
    "lean-build": "rm -rf .next && next build && npm run clean-cache",
    "lean-build-start": "npm run lean-build && npm run start",
    "upload-build": "scp -i ~/.ssh/server2025.pem -r .next ubuntu@13.127.161.131:/home/ubuntu/zecocenter",
    "upload-programs": "scp -i ~/.ssh/server2025.pem -r public/programs ubuntu@13.127.161.131:/home/ubuntu/zecocenter/public/programs",
    "upload-scenes": "scp -i ~/.ssh/server2025.pem -r public/scenes ubuntu@13.127.161.131:/home/ubuntu/zecocenter/public/scenes",
    "upload-media": "scp -i ~/.ssh/server2025.pem -r uploads/media ubuntu@13.127.161.131:/home/ubuntu/zecocenter/uploads/",
    "restart-prod": "ssh -i ~/.ssh/server2025.pem ubuntu@13.127.161.131 'source ~/.nvm/nvm.sh && pm2 restart zecocenter'",
    "deploy": "npm run clean-cache && npm run clean-dist && time next build && npm run clean-cache && time npm run upload-build && npm run restart-prod",
    "logs": "ssh -i ~/.ssh/server2025.pem ubuntu@13.127.161.131 'source ~/.nvm/nvm.sh && pm2 logs --lines 10 --raw zecocenter'",
    "ssh": "ssh -i ~/.ssh/server2025.pem ubuntu@13.127.161.131"
  },
